{
  "expo": {
    "name": "TestApp01",
    "slug": "TestApp01",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.y.konishi.TestApp01"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "package": "com.y.konishi.TestApp01"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [ 
        "react-native-google-mobile-ads" , 
        { 
          "androidAppId" :  "ca-app-pub-2591881801621460~1392941553" ,
          "iosAppId" :  "ca-app-pub-2591881801621460~1971326364"
        } 
      ],
      "expo-font",
      "expo-tracking-transparency",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "008ddf8d-e60e-405f-aace-4df8c57cb9fd"
      }
    }
  }
}